<!-- <?xml version="1.0" encoding="utf-8"?> -->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0099cc"
    tools:context=".GefahrengutResult" >
    
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true" >

        
 		<include
    		android:id="@+id/einsatzinfos"
    		layout="@layout/einsatzinfo" />
      
    
        <ScrollView 
		    android:layout_width="725dp"
		    android:layout_height="fill_parent"
		    android:background="@color/white" >
        
        <LinearLayout
            android:id="@+id/fullscreen_content_controls"
            style="?buttonBarStyle"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:background="@color/white"
            android:orientation="vertical">
	            
	          <TextView android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:text="Gefahrgut"
              android:textColor="@color/black_overlay"
              android:paddingLeft="20dip"
              android:textSize="25dip"
              android:layout_marginTop="30dip"
              android:textStyle="bold"/>
    
     <TextView android:id="@+id/nummer"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:layout_marginTop="20dip"/>
     <TextView android:id="@+id/name"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"/>
     <TextView android:id="@+id/einsatzmassnahme"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Einsatzmaßnahmen"
                    android:textColor="#000000"
                    android:textSize="25dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/brandbekaempfung"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Brandbekämpfung"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/brandbekaempfungBody"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"/>
      <TextView android:id="@+id/leckage"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Leckage"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/leckageBody"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"/>
      <TextView android:id="@+id/erstehilfe"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Erste Hilfe"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/erstehilfeBody"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"/>
      <TextView android:id="@+id/gefahren"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Gefahren"
                    android:textColor="#000000"
                    android:textSize="25dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/brandexplosion"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Brand oder Explosion"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/brandexplosionBody"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"/>
      <TextView android:id="@+id/gesundheit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Gesundheit"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/gesundheitBody"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"/>
      <TextView android:id="@+id/massnahmen"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Maßnahmen"
                    android:textColor="#000000"
                    android:textSize="25dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/sicherheitshinweise"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Sicherheitshinweise"
                    android:textColor="#000000"
                    android:textSize="22dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/anfahren"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Anfahren, Absperren, Sichern, Warnen (Räumen)"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/anfahrenBody"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"/>
      <TextView android:id="@+id/schutzvorkehrung"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Schutzvorkehrungen innerhalb der inneren Absperrung"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="15dip"
                    android:textStyle="bold"/>
      <TextView android:id="@+id/schutzvorkehrungBody"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:paddingLeft="40dip"
                    android:paddingTop="5dip"/>
      
     
    <Button android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Neue Suche"
        android:textSize="20dip"
        android:textStyle="bold"
        android:id="@+id/btnSuche"
        android:layout_marginTop="60dip"/>
            
            
          </LinearLayout>
          </ScrollView>
       
        
        <include
    		android:id="@+id/leisteGefahrgutResult"
    		layout="@layout/leistefire" />
        
   </LinearLayout>
</FrameLayout>


